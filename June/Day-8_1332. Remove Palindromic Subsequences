// Problem Link : https://leetcode.com/problems/remove-palindromic-subsequences/

// Author - gladiator-20

class Solution {
public:
    int removePalindromeSub(string s) {
        int l=0,r=s.length()-1;
        while(l<r){
            if(s[l]!=s[r])return 2;
            l++;r--;
        }
        return 1;
    }
};